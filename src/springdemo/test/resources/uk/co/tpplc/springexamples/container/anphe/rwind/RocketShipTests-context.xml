<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:c="http://www.springframework.org/schema/c"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.1.xsd">


	<bean id="enterprise"
		class="uk.co.tpplc.springexamples.container.anphe.rwind.RocketShip"
		p:engine-ref="warpEngine" p:pilot-ref="picard"
		p:weapons="Phasers,Photon Torpedoes"/>

	<bean id="liberator"
		class="uk.co.tpplc.springexamples.container.anphe.rwind.RocketShip"
		p:engine-ref="ftlEngine" />

	<bean id="warpEngine" class="uk.co.tpplc.springexamples.container.anphe.rwind.Engine"
		p:name="warp" p:fuelType-ref="dilitiumFuel" />

	<bean id="ftlEngine" class="uk.co.tpplc.springexamples.container.anphe.rwind.Engine"
		p:name="ftl" />

	<bean id="picard" class="uk.co.tpplc.springexamples.container.anphe.rwind.Pilot"
		p:surname="Picard" />

	<bean id="dilitiumFuel"
		class="uk.co.tpplc.springexamples.container.anphe.rwind.FuelType"
		p:name="Dilithium Crystals" />
</beans>


