<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="rocketShip" class="uk.co.tpplc.springexamples.danxb.RocketShip">
		<property name="pilot" ref="dan" />
		<!--<property name="Wings" value="X" />
		<property name="Weapons" value="????" /> -->
		<constructor-arg ref="dansEngine"/>
	</bean>

	<bean id="dansEngine" class="uk.co.tpplc.springexamples.danxb.Engine">
		<constructor-arg value="petrol"/>
	</bean>

	<bean id="dan" class="uk.co.tpplc.springexamples.danxb.Pilot">
		<constructor-arg value="Dan"/>
	</bean>
	
	
</beans>