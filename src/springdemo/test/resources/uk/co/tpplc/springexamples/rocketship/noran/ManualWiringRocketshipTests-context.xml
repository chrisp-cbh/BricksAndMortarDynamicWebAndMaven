<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
                  
    <bean id="pilotJason" class="uk.co.tpplc.springexamples.rocketship.noran.Pilot">
		<property name="name" value="Jason"/>
	</bean>
	<bean id="rocketship" class="uk.co.tpplc.springexamples.rocketship.noran.Rocketship">
		<constructor-arg value="Rocket Fuel"/>
		<property name="pilot" ref="pilotDan" />
		<property name="rightEngine" ref="rightEngine" />
		<property name="leftEngine" ref="leftEngine" />
		<property name="wing" ref="wing" />
	</bean>
	<bean id="pilotDan" class="uk.co.tpplc.springexamples.rocketship.noran.Pilot">
		<property name="name" value="Dan"/>
	</bean>
	
	<bean id="rightEngine" class="uk.co.tpplc.springexamples.rocketship.noran.Engine">
		<property name="size" value="6L"/>
	</bean>
	
	<bean id="leftEngine" class="uk.co.tpplc.springexamples.rocketship.noran.Engine">
		<property name="size" value="5L"/>
	</bean>
		<bean id="wing" class="uk.co.tpplc.springexamples.rocketship.noran.Wing">
		<property name="span" value="30ft"/>
	</bean>
</beans>