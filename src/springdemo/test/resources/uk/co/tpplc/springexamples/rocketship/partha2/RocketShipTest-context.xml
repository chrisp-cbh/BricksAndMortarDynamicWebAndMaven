<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="rocketShip" class="uk.co.tpplc.springexamples.rocketship.partha2.RocketShip">
		<property name="engine" ref="arian" />
		<property name="pilot" ref="josh" />
		<property name="weapon" ref="laser" />
		<constructor-arg value="Moon Lander" />
	</bean>

	<bean id="arian" class="uk.co.tpplc.springexamples.rocketship.partha2.ArianRocket" />

	<bean id="spaceshuttle" class="uk.co.tpplc.springexamples.rocketship.partha2.SpaceShuttle" />
	
	<bean id="josh" class="uk.co.tpplc.springexamples.rocketship.partha2.PilotJosh" />
	
	<bean id="laser" class="uk.co.tpplc.springexamples.rocketship.partha2.DualMountedLaser" />

</beans>