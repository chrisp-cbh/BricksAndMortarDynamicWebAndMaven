<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="pilotEd" class="uk.co.tpplc.springexamples.ed.paul.stardestroyer.PilotEd">
		<property name="pilotName" value="Ed Pope" />
	</bean>
	
	<bean id="pilotPaul" class="uk.co.tpplc.springexamples.ed.paul.stardestroyer.PilotPaul">
		<property name="pilotName" value="Paul" />
	</bean>

</beans>